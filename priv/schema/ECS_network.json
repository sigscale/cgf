{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"$id": "http://sigscale.org/schema/cgf/ECS_network",
	"title": "Elastic Common Schema (ECS) Network",
	"description": "The network is defined as the communication path over which a host or network event happens.",
	"type": "object",
	"properties": {
		"network": {
			"type": "object",
			"properties": {
				"application": {
					"description": "Application level name.",
					"type": "string",
					"examples": [
						"rf",
						"ga"
					]
				},
				"protocol": {
					"description": "Application protocol name.",
					"type": "string",
					"examples": [
						"diameter",
						"gtp'"
					]
				}
			}
		}
	}
}
