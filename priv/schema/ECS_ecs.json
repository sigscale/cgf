{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"$id": "http://sigscale.org/schema/cgf/ECS_ecs",
	"title": "Elastic Common Schema (ECS)",
	"description": "ECS specific Meta-information.",
	"type": "object",
	"properties": {
		"ecs": {
			"type": "object",
			"properties": {
				"version": {
					"description": "ECS version this event conforms to.",
					"type": "string",
					"enum": [
						"8.5",
						"8.6"
					]
				}
			},
			"required": [
				"version"
			]
		}
	},
	"required": [
		"ecs"
	]
}
