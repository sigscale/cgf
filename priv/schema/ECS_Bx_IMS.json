			"type": "object",
			"properties": {
				"natureOfAddress": {
					"description": "Indicates the scope of the address value.",
					"type": "string",
					"enum": [
						"unknown",
						"subscriber",
						"national",
						"international",
						"network specific",
						"spare",
						"reserved"
					]
				},
				"numberingPlan": {
					"description": "Describes which numbering plan is used.",
					"type": "string",
					"enum": [
						"unknown",
						"e164",
						"x121",
						"e212",
						"national",
						"private",
						"reserved"
					]
				},
				"address": {
					"description": "The digits of the address.",
					"type": "string",
					"pattern": "^[0-9#*]{1,38}$"
				}
			},
			"required": [
				"address"
			],
			"examples": [
